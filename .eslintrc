{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": 0,
    "comma-dangle": 0,
    "semi": 2,
    "indent": [2, 2],
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-extra-boolean-cast": 0,
    "eol-last": 2,
    "quotes": [2, "single"],
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/prop-types": 1
  }
}
